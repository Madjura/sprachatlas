         <annotationBlock who="#Int" start="#T14" end="#T18">
            <u xml:id="u_d1e1286">
               <w xml:id="w_TIE4_1">Hat man man da</w>
               <anchor synch="#T15" />
               <w xml:id="w_TIE4_2">noch Fischen dürfen?</w>
            </u>
         </annotationBlock>
         <annotationBlock who="#Int" start="#T14" end="#T18">
            <u xml:id="u_d1e1302">
               <w xml:id="w_TIE5_0">ho5-dma2&amp;do6-</w>
               <anchor synch="#T15" />
               <w xml:id="w_TIE5_1">na6+-2vi5s91n4d2e2e,fa,</w>
            </u>
         </annotationBlock>
